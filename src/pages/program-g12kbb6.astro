---
import Program from '@components/program/program.svelte'
import { TimelineComponent } from '@components/timeline/Timeline'
import Layout from '@layouts/Layout.astro'
import { api } from 'src/api'
const [events, blocks, types, places] = await Promise.all([
  api.programEvents.allPublic().then(res => res.data),
  api.programBlocks.getAll().then(res => res.data),
  api.programTypes.all().then(res => res.data),
  api.programPlaces.all().then(res => res.data),
])
---

<Layout title="Program" description="Program SkierConu 2024">
  <Program events={events} client:only="svelte" types={types} blocks={blocks} />
</Layout>
